{
  "name": "github-actions-releaser",
  "version": "2.0.0",
  "description": "This action generate a release notes from closed issues",
  "keywords": [
    "actions",
    "node",
    "publish",
    "release"
  ],
  "homepage": "https://github.com/raulanatol/github-actions-releaser#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/github-actions-releaser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/github-actions-releaser.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist lib",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "@actions/core": "1.3.0",
    "@actions/github": "5.0.0"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "1.2.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.6.0",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "@vercel/ncc": "0.28.5",
    "eslint": "7.27.0",
    "eslint-plugin-jest": "24.3.6",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "js-yaml": "4.1.0",
    "prettier": "2.3.0",
    "shx": "0.3.3",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  }
}
